<?xml version="1.0" ?>
<launch>

  <!--> Red Blue Guard <!-->
  <node pkg="dfrobot_2wd" type="dfrobot_2wd_node" name="$(anon dfrobot_2wd_node_red_blue)" required="true" output="screen">
    <param name="robot_name" value="$(optenv ROS_ROBOT_NAME red_blue)" />
    <param name="port_name" value="$(optenv ROS_PORT_NAME /dev/ttyACM1)" />
  </node>
  
</launch>